    "#!/usr/bin/python3\n",
    "\n",
    "from redpitaya import *\n",
    "import sys\n",
    "\n",
    "base.Init()\n",
    "\n",
    "values = map(float, sys.argv[1:])\n",
    "\n",
    "for i, v in enumerate(values):\n",
    "\tprint(\"pin[%d] = %f\" % (i, v))\n",
    "\tbase.AOpinSetValue(i, v)\n",
    "\n",
    "base.Release()\n"
   ]
  }
